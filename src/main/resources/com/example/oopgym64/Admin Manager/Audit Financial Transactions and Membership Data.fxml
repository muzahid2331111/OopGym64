<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oopgym64.Admin.AuditFinancialTransactionsAndMembershipData">

    <!-- Title -->
    <Label layoutX="14.0" layoutY="14.0" text="Audit Financial Transactions Membership Data" />

    <!-- Table for audit data -->
    <TableView fx:id="auditTable" layoutX="14.0" layoutY="50.0" prefHeight="250.0" prefWidth="570.0">
        <columns>
            <TableColumn fx:id="colDate" prefWidth="100.0" text="Date" />
            <TableColumn fx:id="colType" prefWidth="120.0" text="Type" />
            <TableColumn fx:id="colAmount" prefWidth="100.0" text="Amount" />
            <TableColumn fx:id="colStatus" prefWidth="100.0" text="Status" />
        </columns>
    </TableView>

    <!-- Buttons -->
    <Button fx:id="btnFlag" layoutX="14.0" layoutY="320.0" text="Flag Selected" />
    <Button fx:id="btnExport" layoutX="120.0" layoutY="320.0" text="Export" />
   <Button fx:id="backButtonOnClick" layoutX="510.0" layoutY="345.0" mnemonicParsing="false" text="Back" />

</AnchorPane>
